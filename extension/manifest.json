{
    "name": "Timesheet",
    "version": "2.1",
    "description": "Gets timeline from Google Maps then filters out work hours.",
    "icons": {
      "16": "icon_16.png",
      "32": "icon_32.png",
      "48": "icon_48.png",
      "128": "icon_128.png"
    },
    "background": {
      "persistent": false,
      "scripts": ["background.js"]
    },
    "permissions": [
        "<all_urls>",
        "cookies"
    ],
    "browser_action": {
      "default_icon": "icon_128.png",
      "default_popup": "popup.html"
    },
    "manifest_version": 2
  }